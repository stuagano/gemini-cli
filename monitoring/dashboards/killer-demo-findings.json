{
  "dashboard": {
    "id": null,
    "title": "Killer Demo - Scaling Issues Detection",
    "tags": ["killer-demo", "scaling", "performance", "optimization"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Scaling Issues Detected (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(gemini_scaling_issues_detected_total[24h]))",
            "legendFormat": "Total Issues",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "orange", "value": 15},
                {"color": "red", "value": 30}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Critical Scaling Issues",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(gemini_scaling_issues_detected_total{severity=\"critical\"})",
            "legendFormat": "Critical Issues",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Estimated Cost Savings",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(gemini_scaling_optimizations_cost_savings_usd)",
            "legendFormat": "Cost Savings ($)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 10000}
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Performance Impact",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(gemini_scaling_optimizations_performance_improvement_percent)",
            "legendFormat": "Avg Performance Gain %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 25}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Scaling Issues Detection Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(gemini_scaling_issues_detected_total[5m]) * 60",
            "legendFormat": "Issues detected/min",
            "refId": "A"
          },
          {
            "expr": "rate(gemini_scaling_issues_resolved_total[5m]) * 60",
            "legendFormat": "Issues resolved/min",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 5,
              "fillOpacity": 20
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Scaling Issue Categories",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(gemini_scaling_issues_detected_total) by (issue_type)",
            "legendFormat": "{{issue_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Service Impact Analysis",
        "type": "table",
        "targets": [
          {
            "expr": "sum(gemini_scaling_issues_detected_total) by (service, issue_type)",
            "legendFormat": "{{service}} - {{issue_type}}",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "service": "Service",
                "issue_type": "Issue Type",
                "Value": "Count"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Resource Efficiency Trending",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(gemini_resource_utilization_cpu_percent) by (service)",
            "legendFormat": "CPU - {{service}}",
            "refId": "A"
          },
          {
            "expr": "avg(gemini_resource_utilization_memory_percent) by (service)",
            "legendFormat": "Memory - {{service}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Killer Demo Detection Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "gemini_killer_demo_detection_accuracy_percent",
            "legendFormat": "Detection Accuracy",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "False Positive Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "gemini_killer_demo_false_positive_rate_percent",
            "legendFormat": "False Positive Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 25}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 24}
      },
      {
        "id": 11,
        "title": "Top Scaling Hotspots",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(gemini_scaling_issues_detected_total) by (file_path, function_name))",
            "legendFormat": "{{file_path}}::{{function_name}}",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "file_path": "File",
                "function_name": "Function",
                "Value": "Issues Count"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 12,
        "title": "Optimization Recommendations Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(gemini_optimization_recommendations_total) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 80
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 13,
        "title": "Business Impact Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(gemini_scaling_optimizations_requests_per_second_improvement[5m])",
            "legendFormat": "RPS Improvement",
            "refId": "A"
          },
          {
            "expr": "rate(gemini_scaling_optimizations_latency_reduction_ms[5m])",
            "legendFormat": "Latency Reduction (ms)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ]
  }
}